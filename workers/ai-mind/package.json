{
  "name": "asai-eq-memory",
  "version": "2.0.0",
  "description": "ASAi EQ Memory - Emergent Emotional Intelligence for AI Companions",
  "main": "src/index.ts",
  "scripts": {
    "dev": "wrangler dev",
    "deploy": "wrangler deploy",
    "db:create": "wrangler d1 create asai-eq-memory",
    "db:migrate": "wrangler d1 execute asai-eq-memory --file=./migrations/0001_unified_feelings.sql",
    "db:migrate:local": "wrangler d1 execute asai-eq-memory --local --file=./migrations/0001_unified_feelings.sql",
    "vectorize:create": "wrangler vectorize create asai-eq-memory-vectors --dimensions=768 --metric=cosine",
    "tail": "wrangler tail"
  },
  "keywords": [
    "ai",
    "memory",
    "emotional-intelligence",
    "cloudflare-workers",
    "mcp"
  ],
  "author": "Alex & Fox",
  "license": "MIT",
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20240117.0",
    "typescript": "^5.3.3",
    "wrangler": "^3.24.0"
  }
}
